{
  "name": "gscan",
  "version": "1.4.1",
  "description": "Scans Ghost themes looking for errors, deprecations, features and compatibility",
  "keywords": [
    "ghost",
    "themes",
    "validator",
    "lint"
  ],
  "homepage": "https://ghost.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:TryGhost/gscan.git"
  },
  "engine": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/TryGhost/gscan/issues"
  },
  "author": {
    "name": "Ghost Foundation",
    "email": "hello@ghost.org",
    "web": "https://ghost.org"
  },
  "main": "lib",
  "license": "MIT",
  "scripts": {
    "start": "node app/index.js",
    "dev": "NODE_ENV=development DEBUG=gscan:* nodemon",
    "test": "NODE_ENV=testing mocha -- $(find test -name '*.test.js')",
    "coverage": "NODE_ENV=testing istanbul cover --dir test/coverage _mocha -- $(find test -name '*.test.js')",
    "ship": "np --yolo",
    "deploy": "shipit"
  },
  "bin": {
    "gscan": "./bin/cli.js"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "express": "^4.16.2",
    "express-hbs": "^1.0.3",
    "extract-zip": "^1.6.5",
    "fs-extra": "^0.26.2",
    "ghost-ignition": "^2.8.16",
    "glob": "^7.0.5",
    "lodash": "4.17.4",
    "multer": "^1.1.0",
    "require-dir": "^0.3.2",
    "semver": "^5.3.0",
    "uuid": "^3.0.0",
    "validator": "^6.3.0"
  },
  "devDependencies": {
    "@tryghost/deploy": "0.0.2",
    "istanbul": "^0.4.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.11.0",
    "np": "2.19.0",
    "rewire": "^2.5.2",
    "should": "^7.1.0",
    "sinon": "^1.17.5"
  },
  "files": [
    "lib",
    "bin",
    "app"
  ]
}
